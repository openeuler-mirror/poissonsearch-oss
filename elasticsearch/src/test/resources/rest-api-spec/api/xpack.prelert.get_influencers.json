{
  "xpack.prelert.get_influencers": {
    "methods": [ "GET" ],
    "url": {
      "path": "/_xpack/prelert/results/{jobId}/influencers",
      "paths": [ "/_xpack/prelert/results/{jobId}/influencers" ],
      "parts": {
        "jobId": {
          "type" : "string",
          "required": true
        }
      },
      "params": {
        "includeInterim": {
          "type": "boolean"
        },
        "from": {
          "type": "int",
          "description": "skips a number of influencers"
        },
        "size": {
          "type": "int",
          "description": "specifies a max number of influencers to get"
        },
        "start": {
          "type": "string",
          "description": "start timestamp for the requested influencers",
          "required": true
        },
        "end": {
          "type": "string",
          "description": "end timestamp for the requested influencers",
          "required": true
        },
        "anomalyScore": {
          "type": "double",
          "description": "anomaly score threshold for the requested influencers"
        },
        "sort": {
          "type": "string",
          "description": "sort field for the requested influencers"
        },
        "desc": {
          "type": "boolean",
          "description": "whether the results should be sorted in decending order"
        }
      }
    }
  }
}

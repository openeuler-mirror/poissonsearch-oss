showFunctionsWithExactMatch
SHOW FUNCTIONS LIKE 'ABS';

    name:s      |    type:s
ABS             |SCALAR
;


showFunctionsWithPatternWildcard
SHOW FUNCTIONS LIKE 'A%';

    name:s      |    type:s
AVG             |AGGREGATE
ABS             |SCALAR
ACOS            |SCALAR
ASIN            |SCALAR
ATAN            |SCALAR
;

showFunctionsWithPatternChar
SHOW FUNCTIONS LIKE 'A__';

    name:s      |    type:s
AVG             |AGGREGATE
ABS             |SCALAR
;

